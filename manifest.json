{
    "manifest_version": 2,

    "name": "Minimal New Tab",
    "description": "Minimal New Tab",
    "version": "1.0",

    "chrome_url_overrides": { "newtab": "index.html" },
    
    "browser_action": {},
    
    "permissions": ["tabs"],

    "background": {
        "scripts": ["assets/background.js"],
        "persistent": false
    }
}